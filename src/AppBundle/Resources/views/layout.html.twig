<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8"/>

    <title>{% block title %}FCA Tools Bundle{% endblock %}</title>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8"/>
    <meta name="description" content="FCA Tools Bundle is a website containing a collection of tools that can be used for conceptual structure analysis."/>
    <meta name="keywords" content="FCA,Formal Concept Analysis,Context,Concepts,Dyadic Context,Triadic Context,Concept Finder,FCA Tools Bundle,Tools"/>

    {% block stylesheets %}
        <link href="{{ asset("bundles/app/stylesheets/styles.css") }}" rel="stylesheet" type="text/css"/>

        <link href="{{ asset("bundles/app/libraries/perfect-scrollbar/css/perfect-scrollbar.min.css") }}" rel="stylesheet" type="text/css"/>

        <link href="{{ asset("bundles/app/libraries/data-tables/css/dataTables.bootstrap.min.css") }}" rel="stylesheet" type="text/css"/>

        <link href="{{ asset("bundles/app/stylesheets/main.css") }}" media="screen, projection" rel="stylesheet"
              type="text/css"/>
    {% endblock %}

    <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}"/>

    <script>
        (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
                (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
            m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
        })(window,document,'script','https://www.google-analytics.com/analytics.js','ga');

        ga('create', 'UA-101140459-1', 'auto');
        ga('send', 'pageview');
    </script>
</head>

<body>
{% include "@App/Common/menu.html.twig" %}

<div class="container main-container">
    {% block body %}{% endblock %}
</div>

{% include "@App/Common/footer.html.twig" %}

{% include "@App/Common/spinner.html.twig" %}

{% block javascripts %}
    <script src="{{ asset('bundles/app/libraries/jquery/js/jquery-1.12.1.min.js') }}"></script>
    <script src="{{ asset('bundles/app/libraries/jquery/js/jquery-ui-1.11.4/jquery-ui.min.js') }}"></script>
    <script src="{{ asset('bundles/app/libraries/bootstrap/js/bootstrap.min.js') }}"></script>
    <script src="{{ asset('bundles/app/libraries/d3/js/d3.v3.min.js') }}"></script>
	<script src="https://cdn.rawgit.com/eligrey/canvas-toBlob.js/f1a01896135ab378aa5c0118eadd81da55e698d8/canvas-toBlob.js"></script>
	<script src="https://cdn.rawgit.com/eligrey/FileSaver.js/e9d941381475b5df8b7d7691013401e171014e89/FileSaver.min.js"></script>
    <script src="{{ asset('bundles/app/libraries/perfect-scrollbar/js/perfect-scrollbar.jquery.min.js') }}"></script>
    <script src="{{ asset('bundles/app/libraries/data-tables/js/jquery.dataTables.min.js') }}"></script>
    <script src="{{ asset('bundles/app/libraries/data-tables/js/dataTables.bootstrap.min.js') }}"></script>
    <script src="{{ asset('bundles/app/scripts/main.js') }}"></script>
{% endblock %}
</body>
</html>
